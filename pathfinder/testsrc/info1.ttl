@prefix auto: <http://program/automaton#> .
@prefix help: <http://example.com/> . 
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<brubru> a auto:complex_program_linearfactory ;
	auto:factorypart <testprogram1.py> .

<testprogram1.py> a auto:executable ; 
	auto:uses <1>, <2>, <3> ;
	auto:output <3> .
<1> a auto:argument ;
	auto:index 0 ;
	auto:default 3 ;
	auto:generates <first_term> .
<2> a auto:argument ;
	auto:index 1 ;
	auto:default 4 ;
	auto:keyword '--secint' ;
	auto:generates <second_term> .
<3> a auto:argument ;
	a owl:Thing ; 
	auto:index 2 ;
	auto:default_value <file.data> ;
	auto:keyword '--savefile' ; 
	auto:generates <qq> . 
<qq> a auto:generatabledata ;
	a help:sum .
<first_term> a auto:generatabledata ;
	a help:term .
<second_term> a auto:generatabledata ;
	a help:term .
<file.data> a auto:generatabledata .


help:sum
	rdf:comment "sum of integers" .
help:term
	rdf:comment "parts of a sum" .

