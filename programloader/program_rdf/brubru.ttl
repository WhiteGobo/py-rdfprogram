[ rdf:type owl:Ontology
 ] .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2002/07/owl#hasValue
owl:hasValue rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/owl/owlapi/turtle#hasFather
:hasFather rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasParent ;
           rdf:type owl:FunctionalProperty ;
           rdfs:range :Man .


###  http://www.semanticweb.org/owl/owlapi/turtle#hasMother
:hasMother rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasParent ;
           rdf:type owl:FunctionalProperty ;
           rdfs:range :Woman .


###  http://www.semanticweb.org/owl/owlapi/turtle#hasParent
:hasParent rdf:type owl:ObjectProperty ;
           owl:inverseOf :parentOf ;
           rdfs:domain :Person ;
           rdfs:range :Person .


###  http://www.semanticweb.org/owl/owlapi/turtle#parentOf
:parentOf rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/owl/owlapi/turtle#gender
:gender rdf:type owl:DatatypeProperty ,
                 owl:FunctionalProperty .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/owl/owlapi/turtle#Man
:Man rdf:type owl:Class ;
     owl:equivalentClass [ owl:intersectionOf ( :Person
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :gender ;
                                                  owl:hasValue "male"
                                                ]
                                              ) ;
                           rdf:type owl:Class
                         ] .


###  http://www.semanticweb.org/owl/owlapi/turtle#Person
:Person rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :hasParent ;
                          owl:cardinality "2"^^xsd:nonNegativeInteger
                        ] .


###  http://www.semanticweb.org/owl/owlapi/turtle#Woman
:Woman rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( :Person
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :gender ;
                                                    owl:hasValue "female"
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] .


